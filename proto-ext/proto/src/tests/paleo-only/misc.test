test: $(PROTO) -n 3 "(= 'hello 'world)" -headless -dump-after 2 -NDall -Dvalue -stop-after 2.5
= 1 3 0

test: $(PROTO) -n 3 "(= 'hello 'hello)" -headless -dump-after 2 -NDall -Dvalue -stop-after 2.5
