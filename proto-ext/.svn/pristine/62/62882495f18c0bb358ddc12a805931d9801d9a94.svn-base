
function Neighbor() {
   this.nextNbr = null; // neighbors are stored in a doubly linked list
   this.prevNbr = null;
   
   this.id = null;
   this.data_age = 0;
    // space-time displacement to neighbor
   this.x = 0.0;
   this.y = 0.0;
   this.z = 0.0;
    // data shared by neighbor
   this.imports = new Array();
}
